+    case 4:
+        return PICT_a4;
+    case 8:
+        return PICT_a8;
+    case 15:
+        return PICT_x1r5g5b5;
+    case 16:
+        return PICT_r5g6b5;
+    default:
+    case 24:
+        return PICT_x8r8g8b8;
+#if XORG_VERSION_CURRENT >= 10699900
+    case 30:
+        return PICT_x2r10g10b10;
+#endif
+    case 32:
+        return PICT_a8r8g8b8;
+    }
